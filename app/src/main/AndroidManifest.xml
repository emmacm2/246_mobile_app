<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="training.caboose.caboose">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".AppLogin" />
        <activity android:name=".ViewPositions" />
        <activity android:name=".ViewModules" />
        <activity android:name=".ViewPositionDetails" />
        <activity android:name=".ViewModuleDetails" />
        <activity android:name=".ViewYoutubeModule" />
        <activity android:name=".ViewHtmlModule" />
        <activity android:name=".Quiz"></activity>
    </application>

</manifest>